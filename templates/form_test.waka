<part header>
<h1><var $title></h1>
<hr>

<if $file>
<a href="/<!var $$file{file_url}>" target="_blank"><var $$file{filename}></a><br>

<a href="/<!var $$file{file_url}>" target="_blank">
  <img src="/<!var $$file{thumb_url}>" alt="<var $$file{filename}>" title="<var $$file{filename}>">
</a>

<pre>
<var to_json($file, { pretty =\> 1 })>
</pre>
<hr>
</if>

<form action="/app/post" enctype="multipart/form-data" method="POST">
  <label>File <input type="file" name="file"></label>
  <button type="submit">Submit</button>
</form>
<part footer>
